    "def quiz (n):\n",
    "    rem_nights = 0\n",
    "    \n",
    "    def recurse(n, rem_nights):\n",
    "        if n % 2 == 0:\n",
    "            n = n/2\n",
    "            rem_nights = rem_nights + n\n",
    "        elif n % 3 == 0:\n",
    "            n = 2*(n/3)\n",
    "            rem_nights = rem_nights + n\n",
    "        elif n % 2 != 0 & n % 3 != 0:\n",
    "            n = n - n\n",
    "            rem_nights = rem_nights + n\n",
    "            \n",
    "        if rem_nights != 0:\n",
    "            return recurse(n, rem_nights)\n",
    "        \n",
    "    recurse(n, rem_nights)\n",
    "    \n",
    "    return rem_nights\n",
    "            "
   ]
